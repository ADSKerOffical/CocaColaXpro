local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/lolpoppyus/Roblox-Lua/master/Pop%20UI%20Lib", true))()

local ui = library:Tab("Coca-Cola X pro 1.0")

ui:Button("Get Coca-Cola", function()
    last_pick_sin_mrazy = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame 

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-23, 52, -57)
task.wait()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local actionRadius = 2

game:GetService("CoreGui").PurchasePrompt.Enabled = false

for i, v in pairs(workspace:GetDescendants()) do
    if v:IsA("ClickDetector") then
        local objectPosition = v.Parent.Position
        local playerPosition = humanoidRootPart.Position
        local distance = (objectPosition - playerPosition).magnitude 

        if distance <= actionRadius then
            fireclickdetector(v)
        end
    end
end
task.wait(0.1)
for i = 1, 10 do
    wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-25, 56, -58)
end
task.wait(0.1)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-25, 50, -58)
task.wait(0.1)
game. Players.LocalPlayer.Character.HumanoidRootPart.CFrame = last_pick_sin_mrazy
end)

ui:Button("Spam Cola", function()
while true do
wait(0.1)
    local mouse = game:GetService("Players").LocalPlayer:GetMouse()
local clickPosition = mouse.Hit.Position

local args = {
    [1] = CFrame.new(clickPosition),
    [2] = Vector3.new(clickPosition),
    [3] = false
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("cola v7").SpawnGrenade:FireServer(unpack(args))
end
end)

ui:Button("Mega explosion", function()
for i = 1, 100 do
local args = {
    [1] = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position),
    [2] = Vector3.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position),
    [3] = false
}

game:GetService("Players").LocalPlayer.Character:FindFirstChild("cola v7").SpawnGrenade:FireServer(unpack(args))
end
end)

ui:Button("Anti reclining", function()
    local plr = game.Players.LocalPlayer
local char = plr.Character
  for _, child in pairs(char:GetChildren()) do
    if child:IsA("BasePart") then
      child.Massless = false
      child.CustomPhysicalProperties = PhysicalProperties.new(math.huge, math.huge, math.huge) 
    end
  end
end)

ui:Button("Nuke server", function()
    while true do
wait(0.1)
local Players = game:GetService("Players")

local args = {
    [1] = CFrame.new(),
    [2] = Vector3.new(),
    [3] = false
}

local localPlayer = Players.LocalPlayer

for _, player in pairs(Players:GetPlayers()) do
    if player ~= localPlayer then
        local character = player.Character
        if character then
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart then
                args[1] = CFrame.new(humanoidRootPart.Position)
                args[2] = humanoidRootPart.Position
                game:GetService("Players").LocalPlayer.Character:FindFirstChild("cola v7").SpawnGrenade:FireServer(unpack(args))
            end
        end
    end
end
end
end)

ui:Button("Anti Cola", function()
    local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local localPlayer = Players.LocalPlayer
local character = localPlayer.Character
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local distance = 5  -- Расстояние для отталкивания объектов
local heartbeatConnection

local function pushObjects()
    for _, part in ipairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") and part.Anchored == false and part ~= humanoidRootPart then
            local direction = (part.Position - humanoidRootPart.Position).Unit
            local force = direction * 100  -- Можно изменить силу отталкивания здесь
            part.Velocity = force
        end
    end
end

local function onCharacterAdded(newCharacter)
    character = newCharacter
    humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
    heartbeatConnection = RunService.Heartbeat:Connect(pushObjects)
end

local function onCharacterRemoved()
    if heartbeatConnection then
        heartbeatConnection:Disconnect()
    end
end

if character then
    heartbeatConnection = RunService.Heartbeat:Connect(pushObjects)
else
    localPlayer.CharacterAdded:Connect(onCharacterAdded)
end

localPlayer.CharacterRemoving:Connect(onCharacterRemoved)
end)
